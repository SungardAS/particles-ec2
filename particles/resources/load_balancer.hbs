---
listeners: "[]"
---
{{#resource "m:core" "base" type="AWS::ElasticLoadBalancing::LoadBalancer"}}
"Properties": {
  "Listeners": {{listeners}}
  {{#if accessLoggingPolicy}},"AccessLoggingPolicy": {{accessLoggingPolicy}}{{/if}}
  {{#if appCookieStickinessPolicy}},"AppCookieStickinessPolicy": {{appCookieStickinessPolicy}}{{/if}}
  {{#if availabilityZones}},"AvailabilityZones": {{availabilityZones}}{{/if}}
  {{#if connectionDrainingPolicy}},"ConnectionDrainingPolicy": {{connectionDrainingPolicy}}{{/if}}
  {{#if connectionSettings}},"ConnectionSettings": {{connectionSettings}}{{/if}}
  {{#if crossZone}},"CrossZone": {{helper "m:core" "propertyValue" crossZone}}{{/if}}
  {{#if healthCheck}},"HealthCheck": {{healthCheck}}{{/if}}
  {{#if instances}},"Instances": {{instances}}{{/if}}
  {{#if lbCookieStickinessPolicy}},"LBCookieStickinessPolicy": {{lbCookieStickinessPolicy}}{{/if}}
  {{#if loadBalancerName}},"LoadBalancerName": {{helper "m:core" "propertyValue" loadBalancerName}}{{/if}}
  {{#if policies}},"Policies": {{policies}}{{/if}}
  {{#if scheme}},"Scheme": {{helper "m:core" "propertyValue" scheme}}{{/if}}
  {{#if securityGroups}},"SecurityGroups": {{securityGroups}}{{/if}}
  {{#if subnets}},"Subnets": {{subnets}}{{/if}}
  {{#if tags}},"Tags": {{tags}}{{/if}}
}
{{/resource}}
