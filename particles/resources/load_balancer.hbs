  {{#if loadBalancerName}},"LoadBalancerName": {{helper "m:core" "propertyValue" loadBalancerName}}{{/if}}
  {{#if policies}},"Policies": {{helper "m:core" "propertyValue" policies}}{{/if}}
  {{#if scheme}},"Scheme": {{helper "m:core" "propertyValue" scheme}}{{/if}}
  {{#if securityGroups}},"SecurityGroups": {{helper "m:core" "propertyValue" securityGroups}}{{/if}}
