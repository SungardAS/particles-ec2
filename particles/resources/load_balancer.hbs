listeners: []
---
{{#resource "m:core" "base" type="AWS::ElasticLoadBalancing::LoadBalancer"}}
"Properties": {
  "Listeners": {{helper "m:core" "propertyValue" listeners}}
  {{#if accessLoggingPolicy}},"AccessLoggingPolicy": {{helper "m:core" "propertyValue" accessLoggingPolicy}}{{/if}}
  {{#if appCookieStickinessPolicy}},"AppCookieStickinessPolicy": {{helper "m:core" "propertyValue" appCookieStickinessPolicy}}{{/if}}
  {{#if availabilityZones}},"AvailabilityZones": {{helper "m:core" "propertyValue" availabilityZones}}{{/if}}
  {{#if connectionDrainingPolicy}},"ConnectionDrainingPolicy": {{helper "m:core" "propertyValue" connectionDrainingPolicy}}{{/if}}
