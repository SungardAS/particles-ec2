  {{#if crossZone}},"CrossZone": {{helper "m:core" "propertyValue" crossZone}}{{/if}}
  {{#if healthCheck}},"HealthCheck": {{helper "m:core" "propertyValue" healthCheck}}{{/if}}
  {{#if instances}},"Instances": {{helper "m:core" "propertyValue" instances}}{{/if}}
